<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="385.0" prefWidth="447.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.atc.сontrollers.ControllerImportData">
   <children>
      <RadioButton fx:id="abonentsTableRadioButton" layoutX="54.0" layoutY="48.0" mnemonicParsing="false" prefHeight="23.0" prefWidth="134.0" selected="true" text="Таблица абоненты">
         <toggleGroup>
            <ToggleGroup fx:id="tables" />
         </toggleGroup>
      </RadioButton>
      <RadioButton fx:id="phonesTableRadioButton" layoutX="54.0" layoutY="79.0" mnemonicParsing="false" prefHeight="23.0" prefWidth="134.0" text="Таблица телефоны" toggleGroup="$tables" />
      <RadioButton fx:id="tariffsTableRadioButton" layoutX="54.0" layoutY="108.0" mnemonicParsing="false" prefHeight="23.0" prefWidth="134.0" text="Таблица тарифы" toggleGroup="$tables" />
      <Label layoutX="63.0" layoutY="217.0" prefHeight="17.0" prefWidth="34.0" text="Файл:" />
      <TextField fx:id="pathTxtFld" layoutX="97.0" layoutY="213.0" prefHeight="25.0" prefWidth="294.0" />
      <Button fx:id="selectPathToOpenButton" layoutX="398.0" layoutY="213.0" mnemonicParsing="false" onAction="#buttonClickOpenSaveFileDialog" text="..." />
      <Button fx:id="importButton" layoutX="171.0" layoutY="297.0" mnemonicParsing="false" onAction="#buttonClickImport" text="Импортировать" />
   </children>
</AnchorPane>
